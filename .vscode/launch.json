{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "editor",
			"type": "node",
			"request": "launch",
			"program": "${workspaceFolder}/node_modules/.bin/ts-node",
			"cwd": "${workspaceFolder}",
			"outputCapture": "std",
			"console": "internalConsole",
			"args": [
				"node_modules/.bin/webpack-dev-server",
				"--config",
				"webpack.dev.ts"
			]
		},
	]
}